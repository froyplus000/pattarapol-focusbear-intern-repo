{
  "env": {
    "es2021": true,
    "node": true,
    "browser": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-param-reassign": "off"
  }
}
